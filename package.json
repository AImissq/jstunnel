{
  "name": "jstunnel",
  "version": "0.0.4",
  "description": "Secure tunnels to localhost",
  "license": "MIT",
  "author": "Sviatoslav H",
  "homepage": "https://jstunnel.com",
  "repositoryUrl": "https://github.com/svtslv/jstunnel",
  "repository": {
    "type": "git",
    "url": "https://github.com/svtslv/jstunnel"
  },
  "keywords": [
    "localhost",
    "tunnel",
    "client",
    "http",
    "tcp"
  ],
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.js",
  "bin": {
    "jstunnel": "dist/tunnel-cli.js"
  },
  "scripts": {
    "dev:serve": "(lsof -ti tcp:3800 | xargs kill) && serve -n -p 3800 ./node_modules",
    "dev:watch-tcp": "DEBUG=jstunnel ts-node-dev src/tunnel-cli.ts --port=3800 --type=tcp --tcpPort=23800  --rejectUnauthorized=false --apiUrl=http://localhost:7080",
    "dev:watch-http": "DEBUG=jstunnel ts-node-dev src/tunnel-cli.ts --port=3800 --type=http --subdomain=s3800  --rejectUnauthorized=false --apiUrl=http://localhost:7080",
    "dev:start-http": "DEBUG=jstunnel ts-node src/tunnel-cli.ts --port=3800 --type=http --subdomain=s3800  --rejectUnauthorized=false --apiUrl=http://localhost:7080",
    "dev:watch-prod": "DEBUG=jstunnel ts-node-dev src/tunnel-cli.ts --port=3800 --type=http --subdomain=s3800",
    "dev:start-prod": "npm run build && DEBUG=jstunnel node dist/tunnel-cli.js --port=3800 --type=http --subdomain=s3800",
    "build": "npm run test-all && rm -rf dist && tsc -p tsconfig.build.json",
    "test-all": "npm run type-check && npm run lint && npm run format && npm run test",
    "type-check": "tsc --pretty --noEmit",
    "lint": "eslint '**/*.ts'",
    "format": "prettier --check --debug-check '**/*.ts'",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "publish:npm": "npm run build && npm publish --access public",
    "publish:alpha": "npm run build && npm publish --access public --tag alpha"
  },
  "dependencies": {
    "axios": "^0.24.0",
    "minimist": "^1.2.5"
  },
  "devDependencies": {
    "@types/jest": "^27.0.3",
    "@types/node": "^16.11.11",
    "@typescript-eslint/eslint-plugin": "^5.5.0",
    "@typescript-eslint/parser": "^5.5.0",
    "eslint": "^8.4.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.4.3",
    "prettier": "^2.5.1",
    "serve": "^13.0.2",
    "ts-jest": "^27.0.7",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.5.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "./",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
